@model Nop.Plugin.Misc.Suppliers.Models.SuppliersModel

<div class="card-body">
	<div class="form-group row">
		<div class="col-md-3">
			<nop-label asp-for="Name" />
		</div>
		<div class="col-md-9">
			<nop-editor asp-for="Name" />
			<span asp-validation-for="Name"></span>
		</div>
	</div>
	<div class="form-group row">
		<div class="col-md-3">
			<nop-label asp-for="Email" />
		</div>
		<div class="col-md-9">
			<nop-editor asp-for="Email" />
			<span asp-validation-for="Email"></span>
		</div>
	</div>
	<div class="form-group row">
		<div class="col-md-3">
			<nop-label asp-for="IsActive" />
		</div>
		<div class="col-md-9">
			<nop-editor asp-for="IsActive" />
		</div>
	</div>
</div>



